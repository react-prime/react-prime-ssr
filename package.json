{
  "name": "react-prime-ssr",
  "version": "1.3.0",
  "description": "A starter kit to create comprehensive React apps",
  "author": "Label A",
  "license": "ISC",
  "engines": {
    "node": ">=14.7.0",
    "npm": ">=7.8.0"
  },
  "scripts": {
    "start": "node scripts/development.js",
    "build:nextconfig": "node scripts/esbuild.nextconfig.js",
    "build:acc": "APP_ENV=acceptance node scripts/build.js",
    "build:prod": "APP_ENV=production node scripts/build.js",
    "server": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint --ext .ts --ext .tsx --ext .js src/ server/ scripts/ ./next.config.ts",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "typecheck": "tsc --noEmit",
    "test": "concurrently 'npm run lint' 'npm run typecheck'",
    "gql:gen": "graphql-codegen -r dotenv/config",
    "gql:dev": "APP_ENV=development DOTENV_CONFIG_PATH=./.env.development npm run gql:gen"
  },
  "dependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "contentful": "9.1.5",
    "express": "4.17.2",
    "lodash": "4.17.21",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.7",
    "sharp": "0.29.3",
    "styled-components": "5.3.3",
    "typesafe-actions": "5.1.0"
  },
  "devDependencies": {
    "@graphql-cli/codegen": "2.1.6",
    "@graphql-codegen/schema-ast": "2.4.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@graphql-codegen/typescript-react-query": "3.3.0",
    "@next/bundle-analyzer": "12.0.7",
    "@next/eslint-plugin-next": "12.0.7",
    "@svgr/webpack": "6.1.2",
    "@types/copy-webpack-plugin": "10.1.0",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.178",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.19",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "chokidar": "3.5.2",
    "concurrently": "7.0.0",
    "copy-webpack-plugin": "10.2.0",
    "dotenv": "10.0.0",
    "esbuild": "0.14.10",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-react-app": "7.0.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "graphql-config": "4.1.0",
    "kleur": "4.1.4",
    "next-pwa": "5.4.4",
    "ts-toolbelt": "9.6.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "type-fest": "2.8.0",
    "typescript": "4.5.4",
    "url-loader": "4.1.1",
    "webpack": "5.65.0"
  },
  "keywords": [
    "react",
    "express",
    "react-query",
    "boilerplate",
    "styled-components",
    "ssr",
    "next"
  ]
}
