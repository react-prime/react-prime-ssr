{
  "name": "react-prime-ssr",
  "version": "1.0.0",
  "description": "A starter kit to create comprehensive server side rendered React apps with Redux",
  "scripts": {
    "start": "npm run build:nextconfig; nodemon",
    "build": "npm run build:nextconfig; next build; tsc --project tsconfig.server.json",
    "build:server": "rimraf dist/src/; rimraf serverDist/; tsc --project tsconfig.server.json; npm run server:move; rimraf serverDist/",
    "build:nextconfig": "npm run clean:nextconfig; tsc --project tsconfig.nextconfig.json",
    "server:move": "mkdir -p dist/src/server; mv serverDist/src/server/ dist/src/server/; mv serverDist/config/ dist/src/",
    "server": "NODE_ENV=production node dist/server/index.js",
    "clean": "rimraf dist/",
    "clean:nextconfig": "rimraf next.config.js",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "typecheck": "tsc --noEmit",
    "preinstall": "npx npm-force-resolutions"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "ssr",
    "next"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "4.17.1",
    "lodash": "4.17.19",
    "next": "9.5.2",
    "next-routes": "1.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "5.1.1",
    "ts-toolbelt": "6.9.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.10.1",
    "@svgr/webpack": "5.4.0",
    "@types/copy-webpack-plugin": "6.0.0",
    "@types/express": "4.17.6",
    "@types/lodash": "4.14.155",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/redux": "3.6.0",
    "@types/styled-components": "5.1.0",
    "@types/webpack": "4.41.17",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-styled-components": "1.10.7",
    "copy-webpack-plugin": "6.0.2",
    "eslint": "7.2.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-flowtype": "5.1.3",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "file-loader": "6.0.0",
    "json-loader": "0.5.7",
    "next-offline": "5.0.2",
    "nodemon": "2.0.4",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typesafe-actions": "5.1.0",
    "typescript": "3.9.5",
    "url-loader": "4.1.0",
    "webpack": "4.43.0"
  },
  "resolutions": {
    "minimist": "^1.2.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
