    "file-loader": "6.2.0",
    "next-pwa": "5.4.0",
    "ts-toolbelt": "9.6.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.4.4",
    "url-loader": "4.1.1",
    "webpack": "5.61.0"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "ssr",
    "next"
