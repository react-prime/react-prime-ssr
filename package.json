{
  "name": "react-prime-ssr",
  "version": "1.2.0",
  "description": "A starter kit to create comprehensive server side rendered React apps with Redux",
  "scripts": {
    "start": "npm run build:nextconfig; nodemon",
    "build": "npm run build:nextconfig; next build; tsc --project tsconfig.server.json",
    "build:server": "rimraf dist/src/; rimraf serverDist/; tsc --project tsconfig.server.json; npm run server:move; rimraf serverDist/",
    "build:nextconfig": "npm run clean:nextconfig; tsc --project tsconfig.nextconfig.json",
    "server:move": "mkdir -p dist/src/server; mv serverDist/src/server/ dist/src/server/; mv serverDist/config/ dist/src/",
    "server": "NODE_ENV=production node dist/server/index.js",
    "clean": "rimraf dist/",
    "clean:nextconfig": "rimraf next.config.js",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "typecheck": "tsc --noEmit",
    "postinstall": "npx patch-package"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "ssr",
    "next"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "4.17.1",
    "lodash": "4.17.21",
    "next": "10.2.0",
    "next-redux-wrapper": "6.0.2",
    "next-routes": "1.4.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "5.2.3",
    "ts-toolbelt": "9.6.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@next/bundle-analyzer": "10.2.0",
    "@svgr/webpack": "5.5.0",
    "@types/copy-webpack-plugin": "8.0.0",
    "@types/express": "4.17.11",
    "@types/lodash": "4.14.168",
    "@types/node": "15.0.1",
    "@types/react": "17.0.4",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/redux": "3.6.31",
    "@types/styled-components": "5.1.9",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-styled-components": "1.12.0",
    "copy-webpack-plugin": "8.1.1",
    "eslint": "7.25.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "json-loader": "0.5.7",
    "next-offline": "5.0.5",
    "nodemon": "2.0.7",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typesafe-actions": "5.1.0",
    "typescript": "4.2.4",
    "url-loader": "4.1.1",
    "webpack": "5.36.1"
  }
}
