{
  "name": "react-prime-ssr",
  "version": "1.2.0",
  "description": "A starter kit to create comprehensive server side rendered React apps with Redux",
  "author": "Label A",
  "license": "ISC",
  "engines": {
    "node": ">=14.7.0",
    "npm": ">=7.8.0"
  },
  "scripts": {
    "start": "npm run build:nextconfig; DEV=true node scripts/esbuild.server.js",
    "build": "npm run build:nextconfig; next build; node scripts/esbuild.server.js",
    "build:nextconfig": "node scripts/esbuild.nextconfig.js",
    "server": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "typecheck": "tsc --noEmit",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "express": "4.17.1",
    "lodash": "4.17.21",
    "next": "11.0.1",
    "next-redux-wrapper": "7.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "5.3.0",
    "typesafe-actions": "5.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.14.5",
    "@next/bundle-analyzer": "11.0.1",
    "@svgr/webpack": "5.5.0",
    "@types/copy-webpack-plugin": "8.0.0",
    "@types/express": "4.17.12",
    "@types/lodash": "4.14.170",
    "@types/node": "15.12.4",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-redux": "7.1.16",
    "@types/redux": "3.6.31",
    "@types/styled-components": "5.1.10",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-styled-components": "1.12.0",
    "chokidar": "3.5.2",
    "copy-webpack-plugin": "9.0.0",
    "esbuild": "0.12.9",
    "eslint": "7.29.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "next-offline": "5.0.5",
    "ts-toolbelt": "9.6.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.3.4",
    "url-loader": "4.1.1",
    "webpack": "5.40.0"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "ssr",
    "next"
  ]
}
