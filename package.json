{
  "name": "react-prime-ssr",
  "version": "1.3.0",
  "description": "A starter kit to create comprehensive server side rendered React apps with React Query",
  "author": "Label A",
  "license": "ISC",
  "engines": {
    "node": ">=14.7.0",
    "npm": ">=7.8.0"
  },
  "scripts": {
    "start": "npm run build:nextconfig && concurrently \"APP_ENV=development npm run cf-ts-gen -- -p -i 15\" \"APP_ENV=development next dev\"",
    "build:nextconfig": "node scripts/esbuild.nextconfig.js",
    "build:acc": "APP_ENV=acceptation npm run cs-ts-gen:build && npm run build:app",
    "build:prod": "APP_ENV=production npm run cf-ts-gen:build && npm run build:app",
    "build:app": "npm run build:nextconfig && next build && node scripts/esbuild.server.js",
    "server": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint --ext .ts --ext .tsx --ext .js src/ server/ scripts/ ./next.config.ts",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "typecheck": "tsc --noEmit",
    "test": "concurrently \"npm run lint\" \"npm run typecheck\"",
    "cf-ts-gen": "contentful-typescript-codegen --output src/types/generated/contentful.d.ts",
    "cf-ts-gen:lint": "eslint --fix --quiet src/types/generated/contentful.d.ts",
    "cf-ts-gen:build": "npm run cf-ts-gen && npm run cf-ts-gen:lint"
  },
  "dependencies": {
    "contentful": "9.1.5",
    "express": "4.17.2",
    "lodash": "4.17.21",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.7",
    "sharp": "0.29.3",
    "styled-components": "5.3.3",
    "typesafe-actions": "5.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.16.5",
    "@next/bundle-analyzer": "12.0.7",
    "@next/eslint-plugin-next": "12.0.7",
    "@svgr/webpack": "6.1.2",
    "@types/copy-webpack-plugin": "10.1.0",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.178",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.19",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-styled-components": "2.0.2",
    "chokidar": "3.5.2",
    "concurrently": "6.5.1",
    "contentful-management": "7.47.2",
    "contentful-typescript-codegen": "3.2.3",
    "copy-webpack-plugin": "10.2.0",
    "dotenv": "10.0.0",
    "esbuild": "0.14.9",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-react-app": "7.0.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "next-pwa": "5.4.4",
    "ts-toolbelt": "9.6.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "type-fest": "2.8.0",
    "typescript": "4.5.4",
    "url-loader": "4.1.1",
    "webpack": "5.65.0"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "react-query",
    "boilerplate",
    "babel",
    "styled-components",
    "ssr",
    "next"
  ]
}
