    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "next-offline": "5.0.5",
    "ts-toolbelt": "9.6.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.2.4",
    "url-loader": "4.1.1",
    "webpack": "5.36.1"
  }
}
