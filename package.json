{
  "name": "react-prime-ssr",
  "version": "1.0.0",
  "description": "A starter kit to create comprehensive server side rendered React apps with Redux",
  "scripts": {
    "start": "npm run build:nextconfig && ts-node --project tsconfig.server.json server/index.ts",
    "build": "npm run build:nextconfig && next build src/components && tsc --project tsconfig.server.json",
    "build:server": "rimraf dist/src/ && rimraf serverDist/ && tsc --project tsconfig.server.json && npm run server:move && rimraf serverDist/",
    "build:nextconfig": "npm run clean:nextconfig && tsc --project tsconfig.nextconfig.json",
    "server:move": "mkdir -p dist/src/server && mv serverDist/src/server/ dist/src/server/ && mv serverDist/config/ dist/src/",
    "server": "NODE_ENV=production node dist/server/index.js",
    "clean": "rimraf dist/",
    "clean:nextconfig": "rimraf next.config.js",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "typecheck": "tsc"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "ssr",
    "next"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "4.17.1",
    "lodash": "4.17.14",
    "next": "9.3.1",
    "next-routes": "1.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "redux": "4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@svgr/webpack": "5.2.0",
    "@types/express": "4.17.1",
    "@types/lodash": "4.14.144",
    "@types/next": "8.0.7",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-redux": "7.1.5",
    "@types/redux": "3.6.0",
    "@types/styled-components": "5.0.1",
    "@types/webpack": "4.41.17",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-styled-components": "1.10.0",
    "copy-webpack-plugin": "5.1.1",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "file-loader": "6.0.0",
    "json-loader": "0.5.7",
    "next-offline": "5.0.0",
    "rimraf": "3.0.0",
    "ts-node": "8.7.0",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typesafe-actions": "5.1.0",
    "typescript": "3.8.3",
    "url-loader": "4.0.0",
    "webpack": "4.42.0"
  }
}
