{
  "name": "react-prime-ssr",
  "version": "1.0.0",
  "description": "A starter kit to create comprehensive server side rendered React apps with Redux",
  "scripts": {
    "start": "npm run build:nextconfig && ts-node --project tsconfig.server.json server/index.ts",
    "build": "npm run build:nextconfig && npm run clean && next build src && npm run build:server",
    "build:server": "rimraf dist/src/ && rimraf serverDist/ && tsc --project tsconfig.server.json && npm run server:move && rimraf serverDist/",
    "build:nextconfig": "npm run clean:nextconfig && tsc --project tsconfig.nextconfig.json",
    "server:move": "mkdir -p dist/src/server && mv serverDist/src/server/ dist/src/server/ && mv serverDist/config/ dist/src/",
    "server": "NODE_ENV=production node dist/src/server/server/index.js",
    "clean": "rimraf dist/",
    "clean:nextconfig": "rimraf next.config.js",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "analyze": "BUNDLE_ANALYZE=both npm run build"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "styled-components",
    "ssr",
    "next"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "1.7.4",
    "express": "4.17.1",
    "lodash": "4.17.14",
    "next": "9.1.1",
    "next-routes": "1.4.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "redux": "4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "4.3.2",
    "typesafe-actions": "5.1.0-1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@svgr/webpack": "4.3.0",
    "@types/compression": "1.0.1",
    "@types/express": "4.17.1",
    "@types/next": "8.0.6",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-redux": "7.1.5",
    "@types/redux": "3.6.0",
    "@types/styled-components": "4.1.20",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "babel-eslint": "^8.2.5",
    "babel-loader": "8.0.4",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "1.10.0",
    "copy-webpack-plugin": "5.0.3",
    "eslint": "6.6.0",
    "eslint-config-react-app": "3.0.5",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "7.13.0",
    "file-loader": "^1.1.11",
    "json-loader": "0.5.7",
    "next-offline": "4.0.2",
    "rimraf": "^2.6.2",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.6.4",
    "url-loader": "1.1.2",
    "webpack": "4.25.1"
  }
}
